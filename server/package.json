{
  "name": "server",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate": "dotenv -- sequelize-cli db:migrate:status",
    "migrate:up": "dotenv -- sequelize-cli db:migrate",
    "migrate:down": "dotenv -- sequelize-cli db:migrate:undo",
    "migrate:create": "dotenv -- sequelize-cli migration:generate --name",
    "seed": "dotenv -- sequelize-cli db:seed:all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@sinclair/typebox": "^0.34.38",
    "dotenv": "^17.2.0",
    "fastify": "^5.4.0",
    "mysql2": "^3.14.2",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "dotenv-cli": "^9.0.0",
    "fastify-tsconfig": "^3.0.0",
    "nodemon": "^3.1.10",
    "sequelize-cli": "^6.6.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}